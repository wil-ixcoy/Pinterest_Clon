{
  "name": "autenticacion",
  "version": "1.0.0",
  "description": "API para autenticacion con distintos proveedores",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "serve": "nodemon index.js",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all"
  },
  "keywords": [
    "Node.js",
    "Express",
    "sequelize",
    "javascript",
    "passport.js",
    "Postgres"
  ],
  "author": "Wiliams Alexander Tzoc Ixcoy",
  "license": "MIT",
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "joi": "^17.6.0",
    "pg": "^8.7.3",
    "sequelize": "^6.20.1",
    "sequelize-cli": "^6.4.1"
  }
}
